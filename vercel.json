{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/health",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/.*).*)",
            "destination": "/frontend/dist/$1"
        },
        {
            "source": "/",
            "destination": "/frontend/dist/index.html"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 10
        }
    }
}